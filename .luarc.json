{
  "$schema": "https://raw.githubusercontent.com/LuaLS/vscode-lua/master/setting/schema.json",
  "runtime": {
    "version": "LuaJIT"
  },
  "workspace": {
    "library": ["lua", "$VIMRUNTIME", "${3rd}/luv/library"]
  },
  "diagnostics": {
    "unusedLocalExclude": ["_*"]
  },
  "diagnostics.disable": ["redefined-local"]
}
